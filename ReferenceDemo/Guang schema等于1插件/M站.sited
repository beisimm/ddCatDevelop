    return JSON.stringify(list);
}
function tags_parse1(url, html) {
    var $ = cheerio.load(html);
    var list = [];
    $('.vw-subcatalog-contant .vw-frontsound-container').each(function() {
        var item = $(this);
        var bm = {};
        bm.name = item.find('.vw-frontsound-title').text();
        bm.url = urla(item.children('a').attr('href'));
        bm.logo = item.find('img').attr('data-original');
        bm.newSection = '';
        bm.updateTime = '';
		bm.author = '';
		bm.status = '';		
        list.push(bm);
    });
    return JSON.stringify(list);
}

function search_parse(url, html) {
    var jData = JSON.parse(html);
    var list = [];
    for (var i in jData.info.models[0]) {
        var item = jData.info.models[0][i];
        if (item.id) {
            var bm = {};
            bm.name = item.soundstr;
            bm.url = 'http://www.missevan.com/sound/' + item.id;
            bm.logo = item.front_cover;

            bm.btag = "单期节目";
            list.push(bm);
        }
    }
    return JSON.stringify(list);
}

function book_buildUrl1(url) {
return 'http://www.missevan.com/sound/getsound?soundid='+url.match(/\d+/)[0];
}
var sga={};
function book_parseUrl1(url, html) {
    var id = url.match(/soundid=(\d+)/i)[1];
    if (html.indexOf('soundurl_128') != -1) {
        var mp3 = html.match(/soundurl_128\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
    } else {
        if (html.indexOf('soundurl_64') != -1) {
            var mp3 = html.match(/soundurl_64\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
        } else {
            if (html.indexOf('soundurl_32') != -1) {
                var mp3 = html.match(/soundurl_32\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
            } else {
                if (html.indexOf('soundurl') != -1) {
                    var mp3 = html.match(/soundurl\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
                }
            }
        }
    }
	sga.bg=mp3;
	sga.name= eval("'"+html.match(/soundstr\s?"\s?:\s?"\s?(.+?)\s?"/i)[1]+"'");
	sga.logo= html.match(/front_cover\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
    return 'http://www.missevan.com/sound/getimages?soundid=' + id ;
}
function book_parse1(url, html) {
    var jData = JSON.parse(html);
    var data = {};
    data.bg = 'http://static.missevan.com/'+sga.bg;
    data.name = sga.name;
    data.logo = sga.logo;
    data.list = [];
    for (var i in jData.successVal.images) {
        var item = jData.successVal.images[i];
        if (item[0]) {
            data.list.push({
				url: 'http://static.missevan.com/mimages/' + item[0],
                time:item[1]
            });
        }
    }
	data.list.sort(function(a, b) {
		return a.time - b.time;
	});
    return JSON.stringify(data);
}
function book_buildUrl2(url) {
return 'http://www.missevan.com/albuminfo/'+url.match(/\d+/)[0];
}

function book_parse2(url, html) {
    var $ = cheerio.load(html);
    var data = {};
    data.name = $('#channel_zone_name').text();
    data.author = '';
    data.logo = $('.show_album_cover_image img').attr('src');
    data.intro = $('.show_album_intro span').eq(-1).text().trim();
    data.updateTime = '';
    data.isSectionsAsc = 0;
    data.sections = [];
    $('.channel_content .video-title').each(function() {
        var bm = {
            name: $(this).text(),
            url: urla($(this).attr('href'))
        };
		data.sections.push(bm);
    });
    return JSON.stringify(data);
}
function section_parseUrl(url, html) {
    var id = url.match(/soundid=(\d+)/i)[1];
    if (html.indexOf('soundurl_128') != -1) {
        var mp3 = html.match(/soundurl_128\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
    } else {
        if (html.indexOf('soundurl_64') != -1) {
            var mp3 = html.match(/soundurl_64\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
        } else {
            if (html.indexOf('soundurl_32') != -1) {
                var mp3 = html.match(/soundurl_32\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
            } else {
                if (html.indexOf('soundurl') != -1) {
                    var mp3 = html.match(/soundurl\s?"\s?:\s?"\s?(.+?)\s?"/i)[1];
                }
            }
        }
    }
	sga.bg=mp3;
    return 'http://www.missevan.com/sound/getimages?soundid=' + id ;
}

function section_parse(url, html) {
    var jData = JSON.parse(html);
    var data = {};

    data.bg = 'http://static.missevan.com/'+sga.bg;
    data.list = [];

    for (var i in jData.successVal.images) {
        var item = jData.successVal.images[i];
        if (item[0]) {
            data.list.push({
				url: 'http://static.missevan.com/mimages/' + item[0],
                time:item[1]
            });
        }
    }
	data.list.sort(function(a, b) {
		return a.time - b.time;
	});
    return JSON.stringify(data);
}
