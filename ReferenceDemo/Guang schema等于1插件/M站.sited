            <![CDATA[
function login_check(url, cookies) {
    if (!cookies) {
        return '0';
    }
    if (cookies.indexOf('token=') > 0) {
        return '1';
    } else {
        return '0';
    }
}

function urla(u) {
    var host = "http://www.missevan.com";
    if (u.indexOf("http") < 0) {
        if (u.substr(0, 1) != '/') u = host + '/' + u;
        else u = host + u;
    }
    return encodeURI(u);
}

function hots_parse(url, html) {
    var $ = cheerio.load(html);
    var list = [];
    $('.fc-chooseitem-this_week .vw-right-content-first').each(function() {
        var item = $(this);
        var bm = {};
        bm.name = $(this).find('a').text();
        bm.url = urla(item.find('a').attr('href'));
        bm.logo = item.find('img').attr('src');
        list.push(bm);
    });
    $('.fc-chooseitem-this_week .vw-right-content').each(function() {
        var item = $(this);
        var bm = {};
        bm.name = $(this).find('.vw-rank-title').text();
        bm.url = urla(item.children('a').attr('href'));
        bm.logo = 'http://image.coolapk.com/apk_logo/2015/0602/408649_1433207393_0184.png';
        list.push(bm);
    });
    return JSON.stringify(list);
}
function updates_parse(url, html) {
    var $ = cheerio.load(html);
    var list = [];
